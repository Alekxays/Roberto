{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "globals": {
        "client": "readonly"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "no-self-assign": "off"
    }
}